<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.YdShopGoodsServeMapper">
  <resultMap id="YdShopGoodsServeResult" type="YdShopGoodsServe">
    <id     property="id" column="id" />
    <result property="goodsId"        column="goods_id"     />
    <result property="content"        column="content"     />
    <result property="createTime"        column="create_time"     />
    <result property="updateTime"        column="update_time"     />
    <result property="status"        column="status"     />
    <result property="other1"        column="other1"     />
    <result property="other2"        column="other2"     />
  </resultMap>
  <insert id="insert" parameterType="YdShopGoodsServe" useGeneratedKeys="true" keyProperty="id">
    insert into yd_shop_goods_serve (goods_id, content)
    values (#{goodsId,jdbcType=BIGINT}, #{content,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="YdShopGoodsServeResult">
    select id, goods_id, content, create_time, update_time, `status`, other1, other2
    from yd_shop_goods_serve
    where `status`>-1
  </select>
</mapper>